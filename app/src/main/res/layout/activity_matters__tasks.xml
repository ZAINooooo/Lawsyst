    <ImageView

        android:id="@+id/under_construction"
        android:layout_width="186sp"
        android:layout_height="186sp"
        android:layout_gravity="center"
        android:layout_marginBottom="10sp"
        android:layout_marginTop="85sp"
        android:src="@drawable/under_construction_new"
        />

    <TextView
        android:layout_gravity="center"
        android:gravity="center"
        android:id="@+id/param"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:textColor="#555353"
        android:text="Functionality will be available shortly"/>

</LinearLayout>



