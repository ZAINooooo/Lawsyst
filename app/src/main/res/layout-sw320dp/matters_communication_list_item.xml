    <RelativeLayout
        android:id="@+id/view_foreground"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:padding="10dp">

        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/thumbnail"
            android:layout_width="40dp"
            android:src="@mipmap/no_images"
            android:layout_height="40dp"
            android:scaleType="centerCrop" />



        <TextView
            android:id="@+id/matter_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:maxLength="17"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/thumbnail"
            android:text="ali"
            android:textSize="15dp" />

        <TextView
            android:id="@+id/communicationstatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/matter_name"
            android:text="Draffff"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:maxLength="10"
            android:textColor="#ea3732"
            android:textSize="15dp"
            android:textStyle="bold" />




        <TextView
            android:id="@+id/communicationstatus2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/communicationstatus"
            android:text="Date"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:maxLength="10"
            android:textColor="#ea3732"
            android:textSize="15dp"
            android:textStyle="bold" />






        <ImageView
            android:id="@+id/httpath"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_toRightOf="@id/communicationstatus2"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="13dp"
            app:srcCompat="@mipmap/view" />

    </RelativeLayout>


    <FrameLayout
        android:id="@+id/card"
        android:visibility="gone"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_below="@id/view_foreground"
        android:layout_height="300dp">

        <WebView
            android:id="@+id/web"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </FrameLayout>


</RelativeLayout>





























