    <RelativeLayout
        android:id="@+id/view_foreground"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:padding="10dp">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">
            <WebView
                android:id="@+id/imgChargement"
                android:layout_centerHorizontal="true"
                android:layout_width="match_parent"
                android:layout_height="500dp"
                android:layout_centerInParent="true" />
        </RelativeLayout>


        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/thumbnail"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:scaleType="centerCrop" />



        <TextView
            android:id="@+id/matter_name"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:maxLength="17"
            android:layout_toRightOf="@id/thumbnail"
            android:text="ali"

            android:textSize="15dp" />

        <TextView
            android:id="@+id/communicationstatus"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/matter_name"
            android:layout_marginLeft="30dp"
            android:text="Draffff"
            android:maxLength="10"
            android:textColor="#ea3732"
            android:textSize="15dp"
            android:textStyle="bold" />






        <ImageView
            android:id="@+id/httpath"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/communicationstatus"
            android:layout_marginLeft="105dp"
            app:srcCompat="@mipmap/view" />






    </RelativeLayout>



</FrameLayout>



























